<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="290.0" prefWidth="581.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projetoii.design.administrator.warehouse.data.size.list.FXMLListSizeController">
    <children>
      <TextField fx:id="searchSizeTextField" layoutX="10.0" layoutY="8.0" onKeyPressed="#getSearchList" onKeyReleased="#getSearchList" prefWidth="200.0" promptText="Pesquisar" />
      <Button layoutX="547.0" layoutY="8.0" mnemonicParsing="false" onAction="#handleAddButtonAction" text="+" />
      <TableView fx:id="sizeTable" layoutX="10.0" layoutY="45.0" prefHeight="235.0" prefWidth="561.0">
         <columns>
            <TableColumn fx:id="idColumn" prefWidth="40.0" text="#" />
            <TableColumn fx:id="nameColumn" prefWidth="458.0" text="Nome" />
            <TableColumn fx:id="editColumn" minWidth="0.0" prefWidth="49.0">
               <graphic>
                  <ImageView fitHeight="12.0" fitWidth="12.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@image/edit.png" />
                     </image>
                  </ImageView>
               </graphic></TableColumn>
         </columns>
      </TableView>
   </children>
</AnchorPane>
